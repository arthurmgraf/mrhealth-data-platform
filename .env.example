# MR. HEALTH Data Platform -- Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit this file to version control

# GCP Project Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
GCP_LOCATION=US

# Cloud Storage
GCS_BUCKET_NAME=your-bucket-name

# BigQuery Datasets
BQ_DATASET_BRONZE=mrhealth_bronze
BQ_DATASET_SILVER=mrhealth_silver
BQ_DATASET_GOLD=mrhealth_gold
BQ_DATASET_MONITORING=mrhealth_monitoring

# PostgreSQL Integration (K3s)
PG_HOST=your-k3s-server-ip
PG_SSH_USER=your-ssh-user
PG_DB_NAME=mrhealth
PG_DB_USER=mrh_extractor
PG_DB_PASSWORD=your-pg-password

# Airflow (Docker Compose)
AIRFLOW_DB_USER=airflow
AIRFLOW_DB_PASSWORD=change-me-in-production
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=change-me-in-production

# Superset (Docker Compose)
SUPERSET_SECRET_KEY=generate-with-openssl-rand-hex-32
