    {"id":"cf2_trigger_content","type":"text","x":605,"y":228,"width":190,"height":50,"text":"Type: HTTP (Cloud Scheduler)\nSchedule: 0 4 * * * (01:00 BRT)\nMethod: POST\nAuth: Service Account","fontSize":9,"fontFamily":3,"strokeColor":"#495057"},

